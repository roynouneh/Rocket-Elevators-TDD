  <!-- QUOTE -->
  <section id="quote">
    <div class="container">

      <header class="text-center mb-60">
        <h2>Quote Form</h2>
        <p class="lead font-lato"></p>
        <hr />
      </header>


      <div class="row">

        <!-- FORM -->




        <div class="col-md-12">
          <h3> <strong><em>Select type of building</em></strong></h3>
        </div>


        <form method="post" action="/quotes" id="quoteform">
          <fieldset>
            <%# Company name and email %>
            <div class="row">
              <div class="col-md-6" id="elevator-amount">
                  <label for="input_company_name">
                    Company Name
                    <input type="text" class="form-control" name="quote[company_name]" id="input_company_name">
                  </label>
              </div>
              <div class="col-md-6" id="elevator-amount">
                  <label for="input_email">
                    Email
                    <input type="email" class="form-control" name="quote[email]" id="input_email">
                  </label>
              </div>
            </div>
            
            <!-- Type of building -->
            <div class="row">
              <ul id="building-type">
                <div class="col-md-3">
                  <li>
                    <label class="radio">
                      <input type="radio" name="quote[type_of_building]" value="residential" id="residential">
                      <i></i> Residential
                    </label>
                  </li>
                </div>
                <div class="col-md-3">
                  <li>
                    <label class="radio">
                      <input type="radio" name="quote[type_of_building]" value="commercial" id="commercial">
                      <i></i> Commercial
                    </label>
                  </li>
                </div>
                <div class="col-md-3">
                  <li>
                    <label class="radio">
                      <input type="radio" name="quote[type_of_building]" value="corporate" id="corporate">
                      <i></i> Corporate
                    </label>
                  </li>
                </div>
                <div class="col-md-3">
                  <li>
                    <label class="radio">
                      <input type="radio" name="quote[type_of_building]" value="hybrid" id="hybrid">
                      <i></i> Hybrid
                    </label>
                  </li>
                </div>
              </ul>
            </div>

            <!-- Required variables for calculation -->
            <div class="row">
              <div class="col-md-4" id="number-of-apartments">
                <label for="residential_apartments">Apartments *</label>
                <input onchange="elevatorCalc()"  type="number" name="quote[number_of_apartments]" class="form-control" id="input_number-of-apartments">
              </div>
              <div class="col-md-4" id="number-of-floors">
                <label for="residential_floors">Floors *</label>
                <input onchange="elevatorCalc()"  type="number" name="quote[number_of_floors]" class="form-control" id="input_number-of-floors">
              </div>
              <div class="col-md-4" id="number-of-basements">
                <label for="residential_basements">Basements *</label>
                <input onchange="elevatorCalc()"  type="number" name="quote[number_of_basements]" class="form-control" id="input_number-of-basements">
              </div>
            </div>
            <div class="row">
              <div class="col-md-4" id="number-of-companies">
                <label for="commercial_businesses">Companies *</label>
                <input onchange="elevatorCalc()"  type="number" value="" name="quote[number_of_companies]" class="form-control" id="input_number-of-companies">
              </div>
              <div class="col-md-4" id="number-of-parking-spots">
                <label for="commercial_parking">Parking spaces *</label>
                <input onchange="elevatorCalc()"  type="number" value="" name="quote[number_of_parking_spots]" class="form-control" id="input_number-of-parking-spots">
              </div>
              <div class="col-md-4" id="number-of-elevators">
                <label for="commercial_cages">Elevator cages *</label>
                <input onchange="elevatorCalc()"  type="number" value="" name="quote[number_of_elevators]" class="form-control" id="input_number-of-elevators">
              </div>
            </div>
            <div class="row">
              <div class="col-md-4" id="number-of-corporations">
                <label for="corporate_companies">Corporations *</label>
                <input onchange="elevatorCalc()"  type="number" value="" name="quote[number_of_corporations]" class="form-control" id="input_number-of-corporations">
              </div>
              <div class="col-md-4" id="maximum-occupancy">
                <label for="corporate_cages">Occupants *</label>
                <input onchange="elevatorCalc()"  type="number" value="" name="quote[maximum_occupancy]" class="form-control" id="input_maximum-occupancy">
              </div>
              <div class="col-md-4" id="business-hours">
                <label for="hybrid_activity">Business Hours *</label>
                <input onchange="elevatorCalc()"  type="number" value="" name="quote[business_hours]" class="form-control" id="input_business-hours">
              </div>
            </div>

            <!-- Product Lines -->
            <div class="row">
              <div class="col-md-4">
                <label class="radio">
                  <input onload="serviceCalc()" onchange="serviceCalc()" type="radio" name="quote[product_line]" value="standard" id="standard" checked>
                  <i></i>Standard
                </label>
              </div>
              <div class="col-md-4">
                <label class="radio">
                  <input onchange="serviceCalc()" type="radio" name="quote[product_line]" value="premium" id="premium">
                  <i></i>Premium
                </label>
              </div>
              <div class="col-md-4">
                <label class="radio">
                  <input onchange="serviceCalc()" type="radio" name="quote[product_line]" value="excelium" id="excelium">
                  <i></i>Excelium
                </label>
              </div>
            </div>

            <!-- Results -->
            <div class="row">
              <div class="col-md-4" id="elevator-amount">
                <label for="output_elevator-amount">
                  Elevator amount
                  <input type="text" class="form-control" name="quote[elevator_amount]" id="output_elevator-amount" readonly>
                </label>
              </div>
              <div class="col-md-4" id="elevator-unit-price">
              <label for="">
                Unit Price of Elevators
                <input type="text" value="" name="quote[elevator_unit_price]" class="form-control" id="output_elevator-unit-price" readonly>
              </label>
              </div>
              <div class="col-md-4" id="elevator-total-price">
                <label>
                  Total Price of Elevators
                  <input type="text" value="" name="quote[elevator_total_price]" class="form-control" id="output_elevator-total-price" readonly>
                </label>
              </div>
            </div>
            <div class="row col-md-12" id="installation-fees">
              <label>
                Installation Fees
                <input type="text" value="" name="quote[installation_fees]" class="form-control" id="output_installation-fees" readonly>
              </label>
            </div>
            <div class="row col-md-12" id="final-price">
              <label>
                Final Price
                <input type="text" value="" name="quote[final_price]" class="form-control" id="output_final-price" readonly>
              </label>
            </div>
          </fieldset>


          <div class="row">
            <div>
              <button type="reset" class="btn btn-primary">RESET</button>
            </div>
            <div>
              <button type="submit"  name="commit" value="Create Quote" data-disable-with="Create Quote" class="btn btn-primary">SUBMIT</button>
            </div>
          </div>
        </form>






        <!-- /FORM -->

